## Define Users to be authed by PAM
#Example:
#
<% if $users != 'NONE' -%>
<% users.each do |user| -%>
user = <%=  user %> {
  member = all_access
}
<% end -%>
<% end -%>

key = <%= tac_key %>

# Admin users
group = all_access {
    default service = permit
    login = PAM
    pap   = PAM

    # JunOS related settings
    service = junos-exec {
        local-user-name = remote
        allow-commands = ".*"
        allow-configuration = ".*"
        deny-commands = ""
        deny-configuration = ""
    }
    service = exec {
    priv-lvl = 15
    cisco-av-pair="shell:roles=network-admin"
    }
}

